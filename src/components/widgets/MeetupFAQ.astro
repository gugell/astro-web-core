---
import faqDe from "@/data/about/faq-de.ts";
import faqEn from "@/data/about/faq-en.ts";
import FAQs from "./FAQs.astro";

interface Props {
  t: Record<string, string>;
  lang: string;
}

const { t, lang } = Astro.props;
---

<FAQs
  title={t["faq.title"]}
  subtitle={t["faq.description"]}
  tagline={t["faq.titleShort"]}
  classes={{ container: "max-w-6xl" }}
  items={lang == "en" ? faqEn : faqDe}
/>
